{
    "host": "0.0.0.0",
    "port": 8787,
    "keyLength": 10,
    "maxLength": 400000,
    "staticMaxAge": 86400,
    "recompressStaticAssets": true,
    "logging": [
        {
            "level": "info",
            "type": "Console",
            "colorize": true
        }
    ],
    "keyGenerator": {
        "type": "random"
    },
    "rateLimits": {
        "categories": {
            "normal": {
                "totalRequests": 500,
                "every": 60000
            }
        }
    },
    "storage": {
        "type": "file"
    },
    "documents": {
        "about": "./about.md"
    }
}